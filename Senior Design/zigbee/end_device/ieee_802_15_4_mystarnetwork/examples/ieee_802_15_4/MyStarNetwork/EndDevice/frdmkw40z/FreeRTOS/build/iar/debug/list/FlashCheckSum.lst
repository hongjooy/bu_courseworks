     48          /***********************************************************************
     49          *
     50          *  Function Name    : FlashCheckSum.c
     51          *  Description      : This function is used to calculate checksum value
     52          *                     for the specified flash range.
     53          *  Arguments        : PFLASH_SSD_CONFIG,uint32_t ,uint32_t ,uint32_t*
     54          *  Return Value     : uint32_t
     55          *
     56          ************************************************************************/
     57          
     58          /* Enable size optimization */
     59          #if(ARM_CORTEX_M != CPU_CORE)
     60          #pragma optimize_for_size on
     61          #pragma optimization_level 4
     62          #endif /* End of CPU_CORE */
     63          
     64          uint32_t SIZE_OPTIMIZATION FlashCheckSum(PFLASH_SSD_CONFIG pSSDConfig, \
     65                                          uint32_t dest, \
     66                                          uint32_t size, \
     67                                          uint32_t* pSum)
     68          {
     69              uint32_t counter;          /* Counter for callback operation */
     70              uint32_t data;             /* Data read from Flash address */
     71              uint32_t ret = FTFx_OK;       /* Return code variable */
     72              uint32_t endAddress;       /* P Flash end address */
     73          
     74              counter = 0x0U;
     75              /* convert to byte address */
     76              dest = WORD2BYTE(dest);
     77              /* calculating Flash end address */
     78              endAddress = dest + size;
     79          
     80              /* check for valid range of the target addresses */
     81              if ((dest < WORD2BYTE(pSSDConfig->PFlashBase)) ||
     82                  (endAddress > (WORD2BYTE(pSSDConfig->PFlashBase) + pSSDConfig->PFlashSize)))
     83              {
     84          #if(DEBLOCK_SIZE)
     85                  if ((dest < WORD2BYTE(pSSDConfig->DFlashBase)) ||
     86                  (endAddress > (WORD2BYTE(pSSDConfig->DFlashBase) + pSSDConfig->DFlashSize)))
     87                  {
     88          #endif /* End of if(DEBLOCK_SIZE) */
     89                      /* return an error code FTFx_ERR_RANGE */
     90                      ret = FTFx_ERR_RANGE;
     91                      size = 0x0U;
     92          #if(DEBLOCK_SIZE)
     93                  }
     94          
     95          #endif /* End of if(DEBLOCK_SIZE) */
     96              }
     97              *pSum = 0x0U;
     98              /* doing sum operation */
     99              while(size > 0x0U)
    100              {
    101                  data = READ8(dest);
    102                  *pSum += (uint32_t)data;
    103                  dest += 0x01U;
    104                  size -= 0x01U;
    105          
    106                  /* Check if need to serve callback function */
    107                  if((++counter) >= FLASH_CALLBACK_CS)
    108                  {
    109                      /* serve callback function if counter reaches limitation */
    110                      if(NULL_CALLBACK != pSSDConfig->CallBack)
    111                      {
    112                          (pSSDConfig->CallBack)();
    113                      }
    114                      /* Reset counter */
    115                      counter = 0x0U;
    116                  }
    117              }
    118          
    119          #if C90TFS_ENABLE_DEBUG
    120              /* Enter Debug state if enabled */
    121              if (TRUE == (pSSDConfig->DebugEnable))
    122              {
    123                  ENTER_DEBUG_MODE;
    124              }
    125          #endif
    126              return(ret);
    127          }
