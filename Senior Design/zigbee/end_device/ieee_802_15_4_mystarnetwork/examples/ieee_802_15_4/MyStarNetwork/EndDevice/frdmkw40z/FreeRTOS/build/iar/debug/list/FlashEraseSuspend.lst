     48          /************************************************************************
     49          *
     50          *  Function Name    : FlashEraseSuspend.c
     51          *  Description      : This function is used to suspend a current operation
     52          *                     of flash erase sector command.
     53          *  Arguments        : PFLASH_SSD_CONFIG
     54          *  Return Value     : uint32_t
     55          *
     56          *************************************************************************/
     57          
     58          /* Enable size optimization */
     59          #if(ARM_CORTEX_M != CPU_CORE)
     60          #pragma optimize_for_size on
     61          #pragma optimization_level 4
     62          #endif /* End of CPU_CORE */
     63          
     64          uint32_t SIZE_OPTIMIZATION FlashEraseSuspend(PFLASH_SSD_CONFIG pSSDConfig)
     65          {
     66              uint32_t temp; /* temporary variable */
     67              /* check CCIF bit of the flash status register */
     68              temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FSTAT_OFFSET;
     69              if(0x0U == (REG_BIT_GET(temp, FTFx_SSD_FSTAT_CCIF)))
     70              {
     71                  /* If the command write sequence in progressing, */
     72                  /* Set ERSSUSP bit in FCNFG register */
     73                  temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCNFG_OFFSET;
     74                  REG_BIT_SET(temp, FTFx_SSD_FCNFG_ERSSUSP);
     75          
     76                  temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FSTAT_OFFSET;
     77                  while(0x0U == (REG_BIT_GET(temp, FTFx_SSD_FSTAT_CCIF)))
     78                  {
     79                      /* wait till CCIF bit is set */
     80                  }
     81              }
     82          
     83          #if C90TFS_ENABLE_DEBUG
     84              /* Enter Debug state if enabled */
     85              if (TRUE == (pSSDConfig->DebugEnable))
     86              {
     87                  ENTER_DEBUG_MODE;
     88              }
     89          #endif
     90          
     91              return(FTFx_OK);
     92          }
