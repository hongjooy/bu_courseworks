     48          #ifdef SWAP_M
     49          /************************************************************************
     50          *
     51          *  Function Name    : PFlashSwapCtl
     52          *  Description      : Execute swap command represented by a control code
     53          *
     54          *  Arguments        : PFLASH_SSD_CONFIG, uint32_t, uint8_t,
     55          *                     uint8_t* pCurrentSwapMode,uint8_t* pCurrentSwapBlockStatus,
     56          *                     uint8_t* pNextSwapBlockStatus,
     57          *                     pFLASHCOMMANDSEQUENCE
     58          *
     59          *  Return Value     : uint32_t
     60          *
     61          *************************************************************************/
     62          
     63          /* Enable size optimization */
     64          #if(ARM_CORTEX_M != CPU_CORE)
     65          #pragma optimize_for_size on
     66          #pragma optimization_level 4
     67          #endif /* end of CPU_CORE */
     68          
     69          uint32_t SIZE_OPTIMIZATION PFlashSwapCtl(PFLASH_SSD_CONFIG pSSDConfig,uint32_t addr, uint8_t swapcmd,uint8_t* pCurrentSwapMode, \
     70                                  uint8_t* pCurrentSwapBlockStatus, \
     71                                  uint8_t* pNextSwapBlockStatus, \
     72                                  pFLASHCOMMANDSEQUENCE pFlashCommandSequence)
     73          {
     74              uint32_t ret;      /* Return code variable */
     75              uint32_t temp;     /* temporary variable */
     76          
     77              addr = WORD2BYTE(addr - pSSDConfig->PFlashBase);
     78              /* clear RDCOLERR & ACCERR & FPVIOL flag in flash status register. Write 1 to clear*/
     79              temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FSTAT_OFFSET;
     80              REG_WRITE(temp, FTFx_SSD_FSTAT_ERROR_BITS);
     81          
     82              /* passing parameter to the command */
     83              temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB0_OFFSET;
     84              REG_WRITE(temp, FTFx_PFLASH_SWAP);
     85              temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB1_OFFSET;
     86              REG_WRITE(temp, GET_BIT_16_23(addr));
     87              temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB2_OFFSET;
     88              REG_WRITE(temp, GET_BIT_8_15(addr));
     89              temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB3_OFFSET;
     90              REG_WRITE(temp, GET_BIT_0_7(addr));
     91          
     92              temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB4_OFFSET;
     93              REG_WRITE(temp, swapcmd);
     94              temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB5_OFFSET;
     95              REG_WRITE(temp, 0xFFU);
     96              temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB6_OFFSET;
     97              REG_WRITE(temp, 0xFFU);
     98              temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB7_OFFSET;
     99              REG_WRITE(temp, 0xFFU);
    100          
    101              /* calling flash command sequence function to execute the command */
    102              ret = pFlashCommandSequence(pSSDConfig);
    103          
    104              if (FTFx_OK == ret)
    105              {
    106                  temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB5_OFFSET;
    107                  *pCurrentSwapMode = REG_READ(temp);
    108          
    109                  temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB6_OFFSET;
    110                  *pCurrentSwapBlockStatus = REG_READ(temp);
    111          
    112                  temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB7_OFFSET;
    113                  *pNextSwapBlockStatus = REG_READ(temp);
    114              }
    115          
    116          #if C90TFS_ENABLE_DEBUG
    117              /* Enter Debug state if enabled */
    118              if (TRUE == (pSSDConfig->DebugEnable))
    119              {
    120                  ENTER_DEBUG_MODE;
    121              }
    122          #endif
    123              return (ret);
    124          }
    125          #endif /* End of SWAP_M */
