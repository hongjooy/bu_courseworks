     48          /************************************************************************
     49          *
     50          *  Function Name    : FlashProgramOnce.c
     51          *  Description      : Program a data record into a dedicated 64 bytes
     52          *                        (divided into 16 data records) region in
     53          *                     the P-Flash IFR which stores critical information
     54          *                     for the user
     55          *  Arguments        : PFLASH_SSD_CONFIG, uint8_t , uint8_t*, pFLASHCOMMANDSEQUENCE
     56          *  Return Value     : uint32_t
     57          *
     58          *************************************************************************/
     59          
     60          /* Enable size optimization */
     61          #if(ARM_CORTEX_M != CPU_CORE)
     62          #pragma optimize_for_size on
     63          #pragma optimization_level 4
     64          #endif /* End of CPU_CORE */
     65          
     66          uint32_t SIZE_OPTIMIZATION FlashProgramOnce(PFLASH_SSD_CONFIG pSSDConfig, \
     67                                       uint8_t recordIndex,\
     68                                       uint8_t* pDataArray, \
     69                                       uint32_t size, \
     70                                       pFLASHCOMMANDSEQUENCE pFlashCommandSequence)
     71          {
     72          
     73              uint8_t i;
     74              uint32_t ret;         /* return code variable */
     75              uint32_t temp;        /* temporary variable */
     76          
     77             /* clear RDCOLERR & ACCERR & FPVIOL flag in flash status register. Write 1 to clear*/
     78              temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FSTAT_OFFSET;
     79              REG_WRITE(temp, FTFx_SSD_FSTAT_ERROR_BITS);
     80          
     81              /* passing parameter to the command */
     82              temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB0_OFFSET;
     83              REG_WRITE(temp, FTFx_PROGRAM_ONCE);
     84          
     85              temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB1_OFFSET;
     86              REG_WRITE(temp, recordIndex);
     87          
     88              for (i = 0x0U; i < size; i ++)
     89              {
     90                  temp = pSSDConfig->ftfxRegBase + i + 0x08U;
     91                  REG_WRITE(temp, pDataArray[i]);
     92              }
     93              /* calling flash command sequence function to execute the command */
     94              ret = pFlashCommandSequence(pSSDConfig);
     95          
     96          #if C90TFS_ENABLE_DEBUG
     97              /* Enter Debug state if enabled */
     98              if (TRUE == (pSSDConfig->DebugEnable))
     99              {
    100                  ENTER_DEBUG_MODE;
    101              }
    102          #endif
    103          
    104              return(ret);
    105          }
