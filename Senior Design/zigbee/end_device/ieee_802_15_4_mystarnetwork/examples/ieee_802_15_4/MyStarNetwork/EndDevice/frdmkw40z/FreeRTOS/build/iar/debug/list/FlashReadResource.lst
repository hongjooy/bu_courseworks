     47          /************************************************************************
     48          *
     49          *  Function Name    : FlashReadResource.c
     50          *  Description      : This function is provided for the user to read data
     51          *                     from P-Flash IFR and D-Flash IFR space.
     52          *  Arguments        : PFLASH_SSD_CONFIG, uint32_t, uint8_t*, pFLASHCOMMANDSEQUENCE
     53          *  Return Value     : uint32_t
     54          *
     55          *************************************************************************/
     56          
     57          /* Enable size optimization */
     58          #if(ARM_CORTEX_M != CPU_CORE)
     59          #pragma optimize_for_size on
     60          #pragma optimization_level 4
     61          #endif /* End of CPU_CORE */
     62          
     63          uint32_t SIZE_OPTIMIZATION FlashReadResource(PFLASH_SSD_CONFIG pSSDConfig, \
     64                                                          uint32_t dest, \
     65                                                          uint8_t* pDataArray, \
     66                                                          uint8_t  resourceSelectCode, \
     67                                                          pFLASHCOMMANDSEQUENCE pFlashCommandSequence)
     68          {
     69              uint8_t i;
     70              uint32_t ret = FTFx_OK;       /* return code variable */
     71              uint32_t temp;
     72          
     73                /* convert to byte address */
     74              dest = WORD2BYTE(dest);
     75              /* check if the destination is aligned or not */
     76          #if (DEBLOCK_SIZE)
     77              temp = WORD2BYTE(pSSDConfig->DFlashBase);
     78              if((dest >= temp) && (dest < (temp + pSSDConfig->DFlashSize)))
     79              {
     80                  dest = dest - temp + 0x800000U;
     81              }
     82              else
     83          #endif
     84              {
     85                  temp = WORD2BYTE(pSSDConfig->PFlashBase);
     86                  if((dest >= temp) && (dest < (temp + pSSDConfig->PFlashSize)))
     87                  {
     88                      dest -= temp;
     89                  }
     90                  else
     91                  {
     92                      ret = FTFx_ERR_ACCERR;
     93                  }
     94              }
     95              if(ret == FTFx_OK)
     96              {
     97                 /* clear RDCOLERR & ACCERR & FPVIOL flag in flash status register. Write 1 to clear */
     98                  temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FSTAT_OFFSET;
     99                  REG_WRITE(temp, FTFx_SSD_FSTAT_ERROR_BITS);
    100          
    101                  /* passing parameter to the command */
    102                  temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB0_OFFSET;
    103                  REG_WRITE(temp, FTFx_READ_RESOURCE);
    104                  temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB1_OFFSET;
    105                  REG_WRITE(temp, GET_BIT_16_23(dest));
    106                  temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB2_OFFSET;
    107                  REG_WRITE(temp, GET_BIT_8_15(dest));
    108                  temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB3_OFFSET;
    109                  REG_WRITE(temp, GET_BIT_0_7(dest));
    110          
    111                  temp = pSSDConfig->ftfxRegBase + RSRC_CODE_OFSSET;
    112                  REG_WRITE(temp, resourceSelectCode);
    113          
    114                  /* calling flash command sequence function to execute the command */
    115                  ret = pFlashCommandSequence(pSSDConfig);
    116          
    117                  if (FTFx_OK == ret)
    118                  {
    119                      /* Read the data from the FCCOB registers into the pDataArray */
    120                      for (i = 0x0U; i < PGM_SIZE_BYTE; i ++)
    121                      {
    122                          temp = pSSDConfig->ftfxRegBase + i + 0x08U;
    123                          pDataArray[i] = REG_READ(temp);
    124                      }
    125                  }
    126              }
    127          #if C90TFS_ENABLE_DEBUG
    128              /* Enter Debug state if enabled */
    129              if (TRUE == (pSSDConfig->DebugEnable))
    130              {
    131                  ENTER_DEBUG_MODE;
    132              }
    133          #endif
    134          
    135              return(ret);
    136          }
