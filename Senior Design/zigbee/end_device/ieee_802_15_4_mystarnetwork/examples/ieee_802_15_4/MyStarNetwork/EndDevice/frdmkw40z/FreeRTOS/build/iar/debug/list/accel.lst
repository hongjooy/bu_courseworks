     43          uint8_t accel_read_reg(const accel_dev_t *dev, uint8_t reg_addr)
     44          {
     45              uint8_t outVal = 0;
     46          
     47              if (dev->i2c->i2c_read(dev->slave.base, dev->slave.address, &reg_addr, 1, &outVal, 1) == kStatus_I2C_Success)
     48              {
     49                  return outVal;
     50              }
     51              else
     52              {
     53                  return 0;
     54              }
     55          }
     56          
     57          int32_t accel_write_reg(const accel_dev_t *dev, uint8_t reg_addr, uint8_t reg_val)
     58          {
     59              if (dev->i2c->i2c_write(dev->slave.base, dev->slave.address, &reg_addr, 1, &reg_val, 1) == kStatus_I2C_Success)
     60              {
     61                  return 0;
     62              }
     63              else
     64              {
     65                  return -1;
     66              }
     67          
     68          }
     69          
     70          int32_t accel_init(accel_dev_t *dev)
     71          {
     72              uint8_t val = 0;
     73              int8_t error = 0;
     74              if (!dev)
     75              {
     76                  PRINTF("No Device founded!\r\n");
     77                  error = -1;
     78              }
     79          
     80              if (!dev->i2c)
     81              {
     82                  PRINTF("No I2C interface founded!\r\n");
     83                  error = -1;
     84              }
     85              if(!error)
     86              {
     87                  /* Init i2c */
     88                  dev->i2c->i2c_init(dev->bus, &dev->state);
     89          
     90                  /* Read WHO_AM_I register. */
     91                  val = accel_read_reg(dev, kAccel_WHO_AM_I);
     92          
     93                  switch (val) {
     94                  case kMMA8451_WHO_AM_I_Device_ID:
     95                  dev->accel->accel_init = mma8451_init;
     96                  dev->accel->accel_read_sensor_data = mma8451_read_sensor_data;
     97                  val = ACCEL_MMA8451;
     98                  break;
     99                  case kFXOS_WHO_AM_I_Device_ID:
    100                  dev->accel->accel_init = fxos_init;
    101                  dev->accel->accel_read_sensor_data = fxos_read_sensor_data;
    102                  val = ACCEL_FXOS;
    103                  break;
    104                  default:
    105                  PRINTF("Accel: Unexpected result from WHO_AM_I (0x%02x)\r\n", val);
    106                  error = -1;
    107                  }
    108                  if(!error) dev->accel->accel_init(dev);
    109              }
    110          
    111              return error;
    112          }
