     47          
     48          /*! *********************************************************************************
     49          *************************************************************************************
     50          * Private macros
     51          *************************************************************************************
     52          ********************************************************************************** */
     53          #if gTmrTaskStackSize_c > gSerialTaskStackSize_c
     54          #define gFwkCommonStackSize_c gTmrTaskStackSize_c
     55          #else
     56          #define gFwkCommonStackSize_c gSerialTaskStackSize_c
     57          #endif
     58          
     59          
     60          /*! *********************************************************************************
     61          *************************************************************************************
     62          * Public memory declarations
     63          *************************************************************************************
     64          ********************************************************************************** */
     65          OSA_TASK_DEFINE( FWK, gFwkCommonStackSize_c );
     66          event_t  gFwkCommonEvent;
     67          task_handler_t gFwkCommonTaskId = 0;
     68          
     69          extern const uint8_t gUseRtos_c;
     70          
     71          /*! *********************************************************************************
     72          *************************************************************************************
     73          * Private prototypes
     74          *************************************************************************************
     75          ********************************************************************************** */
     76          extern void SerialManagerTask(task_param_t argument);
     77          extern void TMR_Task(task_param_t param);
     78          static void FwkTask(task_param_t argument);
     79          
     80          
     81          /*! *********************************************************************************
     82          *************************************************************************************
     83          * Public functions
     84          *************************************************************************************
     85          ********************************************************************************** */
     86          void FwkInit(void)
     87          {
     88              osa_status_t status;
     89              
     90              if( gFwkCommonTaskId )
     91                  return;
     92          
     93              status = OSA_EventCreate( &gFwkCommonEvent, kEventAutoClear);
     94              if( kStatus_OSA_Success != status )
     95              {
     96                  panic(0,0,0,0);
     97                  return;
     98              }
     99          
    100              status = OSA_TaskCreate(FwkTask, "FWK_Task", gFwkCommonStackSize_c, FWK_stack,
    101                                      gSerialTaskPriority_c, (task_param_t)NULL, FALSE, &gFwkCommonTaskId);
    102              if( kStatus_OSA_Success != status )
    103              {
    104                  panic(0,0,0,0);
    105                  return;
    106              }
    107          }
    108          
    109          
    110          static void FwkTask(task_param_t argument)
    111          {
    112              while(1)
    113              {
    114                  event_flags_t  ev;
    115                  (void)OSA_EventWait(&gFwkCommonEvent, 0x00FFFFFF, FALSE, OSA_WAIT_FOREVER ,&ev);
    116          
    117          #if gTMR_Enabled_d
    118                  if(ev >> 16)
    119                  {
    120                      TMR_Task(0);
    121                  }
    122          #endif
    123          
    124          #if gSerialManagerMaxInterfaces_c
    125                  ev |= 0x0000FFFF;
    126                  
    127                  if( ev )
    128                  {
    129                      SerialManagerTask(0);
    130                  }
    131          #endif
    132          
    133                  /* For BareMetal break the while(1) after 1 run */
    134                  if (gUseRtos_c == 0)
    135                  {
    136                      break;
    137                  }
    138              }
    139          }
    140          
    141          #endif /* FWK_SMALL_RAM_CONFIG */


 

 


