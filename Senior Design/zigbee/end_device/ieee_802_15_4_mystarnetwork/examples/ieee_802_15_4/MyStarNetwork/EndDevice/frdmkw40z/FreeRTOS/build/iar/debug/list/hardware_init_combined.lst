     34          #include "fsl_debug_console.h"
     35          #include "fsl_ltc_driver.h"
     36          #include "Flash_Adapter.h"
     37          
     38          void hardware_init_combined(void) {
     39          
     40            if((PMC->REGSC & PMC_REGSC_ACKISO_MASK) != 0x00U)
     41            {
     42              PMC->REGSC |= PMC_REGSC_ACKISO_MASK; /* Release hold with ACKISO:  Only has an effect if recovering from VLLSx.*/
     43              /*clear power management registers after recovery from vlls*/
     44              SMC_BWR_STOPCTRL_LLSM(SMC, 0);
     45              SMC_BWR_PMCTRL_STOPM(SMC, 0);
     46              SMC_BWR_PMCTRL_RUNM(SMC, 0);
     47            }
     48            /* enable clock for PORTs */
     49            CLOCK_SYS_EnablePortClock(PORTA_IDX);
     50            CLOCK_SYS_EnablePortClock(PORTB_IDX);
     51            CLOCK_SYS_EnablePortClock(PORTC_IDX);
     52          
     53            /* Init board clock */
     54            BOARD_ClockInit();
     55            //dbg_uart_init();
     56            
     57            
     58            configure_i2c_pins(BOARD_ACCEL_I2C_INSTANCE);
     59            //dbg_uart_init();
     60          
     61          
     62            /* Init DCDC module */
     63            BOARD_DCDCInit();
     64            
     65            /* Install callbacks to handle enter and exit low power */
     66          #if cPWR_UsePowerDownMode
     67            BOARD_InstallLowPowerCallbacks();
     68          #endif
     69            
     70            NV_ReadHWParameters(&gHardwareParameters);
     71          }
