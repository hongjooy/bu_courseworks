     39          int32_t mma8451_init(accel_dev_t *dev)
     40          {
     41              uint8_t val = 0;
     42          
     43              /* Put the mma8451 into standby mode */
     44              val = accel_read_reg(dev, kMMA8451_CTRL_REG1);
     45              val &= (uint8_t)(~(0x01));
     46              accel_write_reg(dev, kMMA8451_CTRL_REG1, val);
     47          
     48              /* Set the range, -4g to 4g */
     49              val = accel_read_reg(dev, kMMA8451_XYZ_DATA_CFG);
     50              val &= (uint8_t)(~0x03);
     51              val |= kMMA8451_4G_Mode;
     52              accel_write_reg(dev, kMMA8451_XYZ_DATA_CFG, val);
     53          
     54              /* Set the F_MODE, disable FIFO */
     55              val = accel_read_reg(dev, kMMA8451_F_SETUP);
     56              val &= 0x3F;
     57              accel_write_reg(dev, kMMA8451_F_SETUP, val);
     58              
     59              // High res
     60              accel_write_reg(dev, kMMA8451_CTRL_REG2, 0x02);
     61              
     62              // Turn on orientation config
     63              accel_write_reg(dev, kMMA8451_PL_CFG_REG, 0x40);
     64          
     65              /* Put the mma8451 into active mode */
     66              val = accel_read_reg(dev, kMMA8451_CTRL_REG1);
     67              val |= 0x01 | 0x04;                 // Activate at max rate, low noise mode
     68              val &= (uint8_t)(~0x02);               /* set F_READ to 0 */
     69              accel_write_reg(dev, kMMA8451_CTRL_REG1, val);
     70          
     71              return 0;
     72          }
     73          
     74          
     75          int32_t mma8451_read_sensor_data(const accel_dev_t * device, accel_sensor_data_t * accel)
     76          {
     77              if (!accel)
     78              {
     79                  return 0;
     80              }
     81          
     82              uint8_t ucStatus = 0;
     83          
     84              do {
     85                  ucStatus = accel_read_reg(device, kMMA8451_STATUS);
     86              } while (!(ucStatus & 0x08));
     87          
     88              accel->mma8451_data.accelXMSB = accel_read_reg(device, kMMA8451_OUT_X_MSB);
     89              accel->mma8451_data.accelXLSB = accel_read_reg(device, kMMA8451_OUT_X_LSB);
     90          
     91              accel->mma8451_data.accelYMSB = accel_read_reg(device, kMMA8451_OUT_Y_MSB);
     92              accel->mma8451_data.accelYLSB = accel_read_reg(device, kMMA8451_OUT_Y_LSB);
     93          
     94              accel->mma8451_data.accelZMSB = accel_read_reg(device, kMMA8451_OUT_Z_MSB);
     95              accel->mma8451_data.accelZLSB = accel_read_reg(device, kMMA8451_OUT_Z_LSB);
     96              
     97              accel->mma8451_data.accelDir = accel_read_reg(device, kMMA8451_PL_STATUS_REG) & 0x7;
     98          
     99              return 1;
    100          }
