     45          /* include the header files */
     46          #include "SSD_FTFx.h"
     47          /************************************************************************
     48          *
     49          *  Function Name    : FlashProgramSection.c
     50          *  Description      : Program data into Flash
     51          *  Arguments        : PFLASH_SSD_CONFIG, uint32_t, uint16_t,
     52          *                     pFLASHCOMMANDSEQUENCE
     53          *  Return Value     : uint32_t
     54          *
     55          *************************************************************************/
     56          #ifndef FTFA_M
     57          
     58          /* Enable size optimization */
     59          #if(ARM_CORTEX_M != CPU_CORE)
     60          #pragma optimize_for_size on
     61          #pragma optimization_level 4
     62          #endif /* End of CPU_CORE */
     63          
     64          uint32_t SIZE_OPTIMIZATION FlashProgramSection(PFLASH_SSD_CONFIG pSSDConfig, \
     65                                                          uint32_t dest, \
     66                                                          uint16_t number, \
     67                                                          pFLASHCOMMANDSEQUENCE pFlashCommandSequence)
     68          {
     69          
     70              uint32_t ret = FTFx_OK;      /* return code variable */
     71              uint32_t temp;
     72          
     73              /* check RAMRDY bit of the flash configuration register */
     74              temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCNFG_OFFSET;
     75              if(0x0U == (REG_BIT_GET(temp, FTFx_SSD_FCNFG_RAMRDY)))
     76              {
     77                  /* return an error code FTFx_ERR_RAMRDY */
     78                  ret = FTFx_ERR_RAMRDY;
     79              }
     80              else
     81              {
     82                  /* convert to byte address */
     83                  dest = WORD2BYTE(dest);
     84          #if (DEBLOCK_SIZE)
     85                  temp = WORD2BYTE(pSSDConfig->DFlashBase);
     86                  if((dest >= temp) && (dest < (temp + pSSDConfig->DFlashSize)))
     87                  {
     88                      dest = dest - temp + 0x800000U;
     89                  }
     90                  else
     91          #endif
     92                  {
     93                      temp = WORD2BYTE(pSSDConfig->PFlashBase);
     94                      if((dest >= temp) && (dest < (temp + pSSDConfig->PFlashSize)))
     95                      {
     96                          dest -= temp;
     97                      }
     98                      else
     99                      {
    100                          ret = FTFx_ERR_ACCERR;
    101                      }
    102                  }
    103          
    104                  if(ret == FTFx_OK)
    105                  {
    106                      temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FSTAT_OFFSET;
    107                      REG_WRITE(temp, FTFx_SSD_FSTAT_ERROR_BITS);
    108          
    109                      /* passing parameter to command */
    110                      temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB0_OFFSET;
    111                      REG_WRITE(temp, FTFx_PROGRAM_SECTION);
    112          
    113                      temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB1_OFFSET;
    114                      REG_WRITE(temp, GET_BIT_16_23(dest));
    115          
    116                      temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB2_OFFSET;
    117                      REG_WRITE(temp, GET_BIT_8_15(dest));
    118          
    119                      temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB3_OFFSET;
    120                      REG_WRITE(temp, GET_BIT_0_7(dest));
    121          
    122                      temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB4_OFFSET;
    123                      REG_WRITE(temp, GET_BIT_8_15(number));
    124          
    125                      temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB5_OFFSET;
    126                      REG_WRITE(temp, GET_BIT_0_7(number));
    127          
    128                      /* calling flash command sequence function to execute the command */
    129                      ret = pFlashCommandSequence(pSSDConfig);
    130                  }
    131              }
    132          
    133          #if C90TFS_ENABLE_DEBUG
    134              /* Enter Debug state if enabled */
    135              if (TRUE == (pSSDConfig->DebugEnable))
    136              {
    137                  ENTER_DEBUG_MODE;
    138              }
    139          #endif
    140          
    141              return(ret);
    142          }
    143          
    144          #endif /* End of FTFA */
