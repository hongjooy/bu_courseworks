     49          /****************************************************************************
     50          *
     51          *  Function Name    : FlashEraseSector
     52          *  Description      : Perform erase operation on Flash
     53          *  Arguments        : PFLASH_SSD_CONFIG, uint32_t, uint32_t, pFLASHCOMMANDSEQUENCE
     54          *  Return Value     : uint32_t
     55          *
     56          *****************************************************************************/
     57          
     58          /* Enable size optimization */
     59          #if(ARM_CORTEX_M != CPU_CORE)
     60          #pragma optimize_for_size on
     61          #pragma optimization_level 4
     62          #endif /* End of CPU_CORE */
     63          
     64          uint32_t SIZE_OPTIMIZATION FlashEraseSector(PFLASH_SSD_CONFIG pSSDConfig, \
     65                                            uint32_t dest, \
     66                                            uint32_t size, \
     67                                            pFLASHCOMMANDSEQUENCE pFlashCommandSequence)
     68          {
     69          
     70              uint32_t ret = FTFx_OK;     /* return code variable */
     71              uint32_t sectorSize;        /* size of one sector */
     72              uint32_t temp;              /* temporary variable */
     73          
     74          
     75              /* convert to byte address */
     76              dest = WORD2BYTE(dest);
     77          
     78          #if (DEBLOCK_SIZE)
     79              temp = WORD2BYTE(pSSDConfig->DFlashBase);
     80              if((dest >= temp) && (dest < (temp + pSSDConfig->DFlashSize)))
     81              {
     82                  dest = dest - temp + 0x800000U;
     83                  sectorSize = FTFx_DSECTOR_SIZE;
     84              }
     85              else
     86          #endif
     87              {
     88                  temp = WORD2BYTE(pSSDConfig->PFlashBase);
     89                  if((dest >= temp) && (dest < (temp + pSSDConfig->PFlashSize)))
     90                  {
     91                      dest -= temp;
     92                      sectorSize = FTFx_PSECTOR_SIZE;
     93                  }
     94                  else
     95                  {
     96                      ret = FTFx_ERR_ACCERR;
     97                      size = 0x0U;
     98                  }
     99              }
    100          
    101              /* check if the size is sector alignment or not */
    102              if(size & (sectorSize - 0x01U))
    103              {
    104                  /* return an error code FTFx_ERR_SIZE */
    105                  ret = FTFx_ERR_SIZE;
    106              }
    107          
    108              while((size > 0x0U) && (FTFx_OK == ret))
    109              {
    110                  /* clear RDCOLERR & ACCERR & FPVIOL flag in flash status register. Write 1 to clear*/
    111                  temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FSTAT_OFFSET;
    112                  REG_WRITE(temp, FTFx_SSD_FSTAT_ERROR_BITS);
    113          
    114                  /* passing parameter to the command */
    115                  temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB0_OFFSET;
    116                  REG_WRITE(temp, FTFx_ERASE_SECTOR);
    117          
    118                  temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB1_OFFSET;
    119                  REG_WRITE(temp, GET_BIT_16_23(dest));
    120          
    121                  temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB2_OFFSET;
    122                  REG_WRITE(temp, GET_BIT_8_15(dest));
    123          
    124                  temp = pSSDConfig->ftfxRegBase + FTFx_SSD_FCCOB3_OFFSET;
    125                  REG_WRITE(temp, GET_BIT_0_7(dest));
    126          
    127                  /* calling flash command sequence function to execute the command */
    128                  ret = pFlashCommandSequence(pSSDConfig);
    129          
    130                  /* update size and destination address */
    131                  size -= sectorSize;
    132                  dest += sectorSize;
    133          
    134              }
    135          
    136          #if C90TFS_ENABLE_DEBUG
    137              /* Enter Debug state if enabled */
    138              if (TRUE == (pSSDConfig->DebugEnable))
    139              {
    140                  ENTER_DEBUG_MODE;
    141              }
    142          #endif
    143          
    144              return(ret);
    145          }
