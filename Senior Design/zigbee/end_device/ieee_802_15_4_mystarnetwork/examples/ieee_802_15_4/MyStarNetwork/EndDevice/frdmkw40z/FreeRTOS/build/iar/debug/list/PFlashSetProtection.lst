     48          /************************************************************************
     49          *
     50          *  Function Name    : PFlashSetProtection.c
     51          *  Description      : This function sets the P-Flash protection to the
     52          *                     intended protection status
     53          *  Arguments        : PFLASH_SSD_CONFIG, uint32_t
     54          *  Return Value     : uint32_t
     55          *
     56          *************************************************************************/
     57          
     58          /* Enable size optimization */
     59          #if(ARM_CORTEX_M != CPU_CORE)
     60          #pragma optimize_for_size on
     61          #pragma optimization_level 4
     62          #endif /* End of CPU_CORE */
     63          
     64          uint32_t SIZE_OPTIMIZATION PFlashSetProtection(PFLASH_SSD_CONFIG pSSDConfig, \
     65                                                uint32_t protectStatus)
     66          {
     67              uint32_t ret = FTFx_OK;
     68              uint32_t addr;
     69              uint32_t temp0, temp1, temp2, temp3;
     70              uint8_t reg0, reg1, reg2, reg3;
     71          
     72              reg0 = GET_BIT_24_31(protectStatus);
     73              reg1 = GET_BIT_16_23(protectStatus);
     74              reg2 = GET_BIT_8_15(protectStatus);
     75              reg3 = GET_BIT_0_7(protectStatus);
     76          
     77              addr = pSSDConfig->ftfxRegBase + FTFx_SSD_FPROT0_OFFSET;
     78              REG_WRITE(addr, reg0);
     79              temp0 = REG_READ(addr);
     80              addr = pSSDConfig->ftfxRegBase + FTFx_SSD_FPROT1_OFFSET;
     81              REG_WRITE(addr, reg1);
     82              temp1 = REG_READ(addr);
     83              addr = pSSDConfig->ftfxRegBase + FTFx_SSD_FPROT2_OFFSET;
     84              REG_WRITE(addr, reg2);
     85              temp2 = REG_READ(addr);
     86              addr = pSSDConfig->ftfxRegBase + FTFx_SSD_FPROT3_OFFSET;
     87              REG_WRITE(addr, reg3);
     88              temp3 = REG_READ(addr);
     89          
     90              /* Read the value of FPPROT registers */
     91              if ((temp0 != reg0) || (temp1 != reg1) || (temp2 != reg2) || (temp3 != reg3))
     92              {
     93                  ret = FTFx_ERR_CHANGEPROT;
     94              }
     95          
     96          #if C90TFS_ENABLE_DEBUG
     97              /* Enter Debug state if enabled */
     98              if (TRUE == (pSSDConfig->DebugEnable))
     99              {
    100                  ENTER_DEBUG_MODE;
    101              }
    102          #endif
    103          
    104              return(ret);
    105          }
